<!DOCTYPE html>
<html lang="es">
<%- include('partials/head.ejs') %>

    <body>
        <%- include('partials/header.ejs') %>

    <h1>Bienvenido Administrador!!!</h1>
    <section>
        <table class="table">
            <thead>
              <tr>
                <th>Nombre</th>
                <th>Precio</th>
                <th>Categoria</th>
            </tr>
            <%= products.forEach(product => { %>
              <tr>
                <th scope="col"><%= product.name %> </th>
                <th scope="col"><%= product.price %> </th>
                <th scope="col"><%= product.category %> </th>
              </tr>
              <% }) %>
            </thead>
          </table>
    </section>
</body>
</html>



